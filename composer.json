{
  "name": "purchase-order",
  "description": "Ecommerce purchase order system with Redis integration",
  "type": "project",
  "require": {
    "php": ">=7.5 <=8.4",
    "yahongie2014/purchaseorder": "dev-main"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/yahongie2014/PurchaseOrder"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
