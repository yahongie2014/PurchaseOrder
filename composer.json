{
  "name": "yahongie2014/purchase-order",
  "description": "Reusable Laravel package",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=7.4 <8.5",
    "laravel/nova": "*",
    "optimistdigital/nova-translatable": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "PurchaseOrder\\": "src/",
      "PurchaseOrder\\Models\\": "src/Models/",
      "PurchaseOrder\\Http\\Resources\\": "src/Http/Resources/",
      "PurchaseOrder\\Database\\Factories\\": "database/factories/",
      "PurchaseOrder\\Database\\Seeders\\": "database/seeders/"
    }
  },
  "repositories": [
    {
      "type": "path",
      "url": "../purchase-order"
    },
    {
      "type": "path",
      "url": "src/nova-src"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "PurchaseOrder\\PurchaseOrderServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
